<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rsa.androidnativobench">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.performanceRelated.CpuIntensiveTask" />
        <activity android:name=".activitys.performanceRelated.MemoryManagmentTask" />
        <activity android:name=".activitys.performanceRelated.Networking" />
        <activity android:name=".activitys.routineTasks.Login" />
        <activity android:name=".activitys.routineTasks.LoadImagesFromDisk" />
        <activity android:name=".activitys.routineTasks.LoadImagesFromNetwork" />
        <activity android:name=".activitys.performanceRelated.CpuIntensiveTaskRx"></activity>
    </application>

</manifest>